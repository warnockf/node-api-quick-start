    "url": "https://github.com/warnockf/node-api-quick-start/issues"
  },
  "homepage": "https://github.com/warnockf/node-api-quick-start#readme",
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-typescript": "^7.9.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "body-parser": "^1.19.0",
    "core-js": "^3.6.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
