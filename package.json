{
  "name": "api-quick-start",
  "version": "1.0.0",
  "description": "A NodeJS API template that uses TypeScript.",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "NODE_ENV=development nodemon",
    "build": "npm run clean-build && npx babel ./src --ignore '**/*.spec.ts' --source-maps -d ./dist --extensions '.ts'",
    "clean-build": "rm -rf ./dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
